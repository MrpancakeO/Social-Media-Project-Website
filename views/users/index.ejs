<h2 class="center">User Table</h2>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
            <th>Gender</th>
            <th>Location</th>
            <th>Email</th>
            <th>Password</th>
            <th>Confirm Password</th>
            <th>Date of Birth</th>
            <th>Security Question</th>
            <th>Answer to Question</th>
            <th>Bio</th>


            <th>Edit</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td>
                    <a href="<%= `/users/${user._id}` %>">
                        <%= user.fname %>
                    </a>
                </td>
                <td>
                    <%= user.lname %>
                </td>
                <td>
                    <%= user.username %>
                </td>
                <td>
                    <%= user.gender %>
                </td>
                <td>
                    <%= user.location %>
                </td>
                <td>
                    <%= user.email %>
                </td>
                <td>
                    <%= user.namepassword %>
                </td>
                <td>
                    <%= user.namepasswordConfirm %>
                </td>
                <td>
                    <%= user.dob %>
                </td>
                <td>
                    <%= user.questions %>
                </td>
                <td>
                    <%= user.question1 %>
                </td>
                <td>
                    <%= user.namebio %>
                </td>

            <!--Edit/Delete Links-->                
                <td>
                    <a href="<%= `users/${user._id}/edit` %>">
                        Edit
                    </a>
                </td>
                <td>
                    <a href="<%= `users/${user._id}/delete?_method=DELETE` %>" onclick="return confirm(`Are you sure?`)">
                        Delete
                    </a>
                </td>
            </tr>
       <% }) %>
    </tbody>
</table>