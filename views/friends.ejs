<div class="container">
    <div class="col-sm-6">
        
    </div>
    <div class="col-sm-6">
        <h1>List of Friends</h1>
        <br>

        <% users.forEach(user => { %>
            
        <li>
            <a href="<%= `/users/${user._id}` %>">
                <%= user.fname + " (" + user.username +") " + user.lname %>
            </a>
            <p>&nbsp<%= user.namebio %></p>

        </li>


        <% }) %>
    </div>

</div>